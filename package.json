{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "tsc && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "tsc && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-react": "^7.10.0",
        "@babel/preset-typescript": "^7.9.0",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "axios": "^0.19",
        "cross-env": "^7.0",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "mini-css-extract-plugin": "^0.9.0",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "typescript": "^3.9.3",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.1.9",
        "@inertiajs/inertia-react": "^0.1.6",
        "laravel-mix-react-typescript-extension": "^1.0.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "tailwindcss": "^1.4.6"
    }
}
